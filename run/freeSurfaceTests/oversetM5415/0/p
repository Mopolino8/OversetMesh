FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    oversetFaces
    {
        type overset;
        value uniform 0;
        coupledFringe yes;

        setHoleCellValue  yes;
        holeCellValue     0;
    }
    bottom
    {
        type zeroGradient;
    }
    hull
    {
        type zeroGradient;
    }
    bilgeKeel
    {
        type zeroGradient;
    }
    deck
    {
        type zeroGradient;
    }
    inlet
    {
        type zeroGradient;
    }
    outlet
    {
        type zeroGradient;
    }
    side
    {
        type zeroGradient;
    }
    symmetry
    {
        type symmetryPlane;
    }
    top
    {
        type zeroGradient;
    }
    x_max
    {
        type zeroGradient;
    }
    x_min
    {
        type zeroGradient;
    }
    y_max
    {
        type symmetryPlane;
    }
    y_min
    {
        type zeroGradient;
    }
    z_max
    {
        type zeroGradient;
    }
    z_min
    {
        type zeroGradient;
    }
}


